{
    "manifest_version": 3,
    "name": "Memory Usage Monitor",
    "version": "1.0",
    "description": "Monitor memory usage of your system",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "permissions": [
      "system.memory"
    ]
  }
  